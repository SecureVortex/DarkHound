# DarkHound Environment Variables Example
# Copy this file to .env and set your actual values
# IMPORTANT: Never commit the actual .env file to version control!

# Alerting Configuration
DARKHOUND_EMAIL_TO=security-team@example.com
DARKHOUND_SLACK_WEBHOOK=https://hooks.slack.com/services/YOUR/SLACK/WEBHOOK
DARKHOUND_TEAMS_WEBHOOK=https://outlook.office.com/webhook/YOUR/TEAMS/WEBHOOK

# Threat Intelligence API Keys
DARKHOUND_HAVEIBEENPWNED_API_KEY=your-hibp-api-key-here
DARKHOUND_DEHASHED_API_KEY=your-dehashed-api-key-here
DARKHOUND_DARKOWL_API_KEY=your-darkowl-api-key-here  
DARKHOUND_FLARE_API_KEY=your-flare-api-key-here
DARKHOUND_CYBERSIXGILL_API_KEY=your-cybersixgill-api-key-here

# Database Configuration (optional override)
DARKHOUND_DB_PATH=/secure/path/to/darkhound.db

# Security Settings (optional)
DARKHOUND_MAX_TIMEOUT=30
DARKHOUND_LOG_LEVEL=INFO